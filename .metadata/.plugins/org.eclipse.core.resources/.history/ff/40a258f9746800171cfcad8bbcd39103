package Spring.MongoSpring;

import java.util.ArrayList;


public interface CsvToCollection 
{
	
	public ArrayList<Country> createCountryList(String filepath , ArrayList<Country> list, Country obj);
	public ArrayList<State> createStatesList(String filepath , ArrayList<State> list, State obj);
	public ArrayList<City> createCityList(String filepath , ArrayList<City> list, City obj);

}
