package Spring.MongoSpring;

import java.util.HashSet;
import java.util.List;

public interface DBService {
	
	public void start();
	public <E> List<E> getCountry(String filePath);
	public <E> HashSet<E> getState(String filePath);
	public <E> List<E> getCity(String filePath);
	public <E> boolean writeCountry(String collection , List<E> list);
	public <E> boolean writeState(String collection , HashSet<E> list);
	public <E> boolean writeCity(String collection , List<E> list);
	public void stop();

}