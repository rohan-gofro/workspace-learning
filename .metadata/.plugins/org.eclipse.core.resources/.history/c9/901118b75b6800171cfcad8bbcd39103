package Gofro.Database;

public class State {
	
	private String id;
	private String name;
	private String countryName;
	private String countryCode;
	
	public State(String id , String name , String countryName , String countryCode)
	{
		this.id = id;
		this.name = name;
		this.countryName = countryName;
		this.countryCode = countryCode;		
	}
	
	public String getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public String getCountryName() {
		return countryName;
	}
	public String getCountryCode() {
		return countryCode;
	}

}