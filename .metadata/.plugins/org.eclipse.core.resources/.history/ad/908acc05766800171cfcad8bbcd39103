package Spring.MongoSpring;

import java.util.ArrayList;
import java.util.HashSet;

public class GofroDataCsvToList implements ObjectToCollection {



	public boolean createCountryList(String filepath, ArrayList<Country> listCountry, Country obj) {
		
		if(listCountry.add(obj))
			return true;
		else
			return false;		
	}

	public boolean createStatesList(String filepath, HashSet<State> listState, State obj, int id) {
		
		if(listState.add(obj))
			return true;
		else
			return false;
		
	}

	public boolean createCityList(String filepath, ArrayList<City> listCity, City obj) {
		
		if(listCity.add(obj))
			return true;
		else
			return false;
		
	}

	

}
