package Spring.MongoSpring;

import org.springframework.beans.factory.BeanFactory;
import org.springframework.beans.factory.xml.XmlBeanFactory;
import org.springframework.core.io.ClassPathResource;
import org.springframework.core.io.Resource;

class App
{
	public static void main(String[] args) {
		
		Resource resouce = new ClassPathResource("Beans.xml");
		BeanFactory factory = new XmlBeanFactory(resouce);
		MongoDbService service = (MongoDbService)factory.getBean("MongoDbBean");		
		GofroDataToCollection data = new GofroDataToCollection();
		
		
	}
}