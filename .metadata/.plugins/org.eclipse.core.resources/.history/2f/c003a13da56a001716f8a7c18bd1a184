package Spring.MongoSpring;

import java.util.ArrayList;


class App
{
	public static void main(String[] args) {
		
		GofroDataToCollection obj = new GofroDataToCollection();
		MongoDbService service = new MongoDbService();		
	
		ArrayList<City> city = obj.createCityList("states.csv", service);
		
		if(service.writeCity(city))
		{
			System.out.println("Success");
		}
		else
		{
			System.out.println("Failed");
		}
		
	}
}