package Spring.MongoSpring;

import java.util.ArrayList;

class App
{
	public static void main(String[] args) {
		
		GofroDataToCollection obj = new GofroDataToCollection();
		MongoDbService service = new MongoDbService();
		ArrayList<City> cityList = obj.createCityList("states.csv", service);
		service.stopService();
		for(City i: cityList)
		{
			i.displayCity();
		}
	
		
		
	}
}